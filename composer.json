{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "acme/multiselect-field": "*",
        "acme/projecs": "*",
        "ajhaupt7/image-upload-preview": "^1.0",
        "averotech/link": "*",
        "averotech/tree": "*",
        "coderello/laravel-nova-lang": "^1.8",
        "eminiarts/nova-tabs": "^1.4",
        "epartment/nova-dependency-container": "^1.3",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "halimtuhu/array-images": "^1.1",
        "laravel/framework": "^8.75",
        "laravel/nova": "*",
        "laravel/sanctum": "^2.11",
        "laravel/tinker": "^2.5",
        "optimistdigital/nova-settings": "^3.5",
        "optimistdigital/nova-tailwind": "^1.1",
        "whitecube/nova-google-maps": "^0.0.1"
    },
    "require-dev": {
        "facade/ignition": "^2.5",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^5.10",
        "phpunit/phpunit": "^9.5.10"
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/MultiselectField"
        },
        {
            "type": "path",
            "url": "./nova-components/Link"
        },
        {
            "type": "path",
            "url": "./nova-components/Tree"
        },
        {
            "type": "path",
            "url": "./nova-components/Projecs"
        }
    ],
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
