{
    "name": "averotech/tree",
    "description": "A Laravel nova field.",
    "keywords": [
        "laravel",
        "Nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-4": {
            "Averotech\\Tree\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Averotech\\Tree\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
